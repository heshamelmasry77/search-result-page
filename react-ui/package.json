{
  "name": "SearchResults-front-end",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/react": "^16.0.25",
    "axios": "^0.18.0",
    "flow-bin": "^0.57.3",
    "history": "^4.7.2",
    "install": "^0.10.1",
    "lint-staged": "^4.3.0",
    "lodash": "^4.17.10",
    "node-sass-chokidar": "1.5.0",
    "npm": "^7.0.0",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.8.2",
    "prop-types": "^15.6.2",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "react-icons": "^2.2.7",
    "react-lazyload": "^2.3.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "4.0.0",
    "react-skylight": "^0.5.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "yarn run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "serve": "yarn npm-run-all -p watch-css start-js",
    "heroku-postbuild": "yarn --production=true && yarn run build",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "flow": "flow"
  },
  "author": "Hesham El Masry <heshamelmasry@outlook.com>",
